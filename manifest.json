{
  "manifest_version": 2,
  "name": "Web Extensions lastError checker",
  "description": "This extension checks the lastError feature availability in FF",
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "webext-feature-checker@avira.com"
    }
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"]
  }],
  "permissions": [
    "*://*/*"
  ]
}
